http = require 'http'



exports=(code)->
  http.get "http://hq.sinajs.cn/list=#{code}",(res)->
    res.on 'data',(data)->
      console.log data.toString()
      content=data.toString().split("=")[1]
      console.log content
    console.log("Got response: " + res.statusCode)
  .on 'error',(e)->
    console.log("Got error: " + e.message)
exports("sh600167")
